<table class="ui table">
  <thead>
    <tr>
       <th>Date</th>
       <th>Weight</th>
       <th>Chest</th>
       <th>Thigh</th>
       <th>Upper Arm</th>
       <th>Waist</th>
       <th>Hips</th>
       <th>Comment</th>
       <th></th>
       <th></th>
    </tr>
  </thead>
  <tbody>
    {{#each assessments}}
    <tr>
      <td>
        {{date}}
      </td>
      <td>
        {{weight}}
      </td>
      <td>
        {{chest}}
      </td>
      <td>
        {{thigh}}
      </td>
      <td>
        {{upperarm}}
      </td>
      <td>
        {{waist}}
      </td>
      <td>
        {{hips}}
      </td>
      <td>
        {{comment}}
      </td>
      <td>
        <a href="/dashboard/{{../member.id}}/deleteassessment/{{id}}" class="ui red icon button">
         <i class="icon trash"></i>        
        </a>
        </td>
    {{/each}}
    
  </tbody>
</table>